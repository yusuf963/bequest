{
  "name": "bequest",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=8.15.0",
    "yarn": "please-use-npm"
  },
  "dependencies": {
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.51",
    "@types/react": "^18.0.17",
